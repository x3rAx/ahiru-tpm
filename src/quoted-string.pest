quoted_string = { single_quoted | double_quoted }

single_quoted =  { "'" ~ single_inner ~ "'" }
single_inner  = @{ (!"'" ~ ANY)* }

double_quoted =  { "\"" ~ double_inner ~ "\"" }
double_inner  = @{ (escaped_char | normal_char)* }

escaped_char = { "\\" ~ ("\"" | "\\") }
normal_char  = { !("\\" | "\"") ~ ANY }
